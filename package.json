{
  "name": "generator-jhipster-generator-jhipster-primeng-blueprint",
  "version": "9.0.0",
  "description": "This blueprint generated code that uses primeng components instead of bootstrap ones",
  "keywords": [
    "yeoman-generator",
    "jhipster-blueprint",
    "jhipster-8"
  ],
  "homepage": "https://github.com/tailosoft/jhipster-primeng-blueprint",
  "license": "Apache-2.0",
  "author": {
    "name": "Youssef El Houti",
    "email": "youssef.elhouti@tailosoft.com",
    "url": "https://tailosoft.com"
  },
  "type": "module",
  "imports": {
    "#test-utils": "./test/utils.mjs"
  },
  "files": [
    "generators",
    "!**/__*",
    "!**/*.snap",
    "!**/*.spec.?(c|m)js"
  ],
  "scripts": {
    "ejslint": "ejslint generators/**/*.ejs",
    "lint": "eslint .",
    "lint-fix": "npm run ejslint && npm run lint -- --fix",
    "prepare": "husky",
    "prettier-check": "prettier --check \"{,**/}*.{md,json,yml,html,cjs,mjs,js,cts,mts,ts,tsx,css,scss,vue,java}\"",
    "prettier-format": "prettier --write \"{,**/}*.{md,json,yml,html,cjs,mjs,js,cts,mts,ts,tsx,css,scss,vue,java}\"",
    "pretest": "npm run prettier-check && npm run lint",
    "test": "vitest run",
    "update-snapshot": "vitest run --update",
    "vitest": "vitest"
  },
  "dependencies": {
    "generator-jhipster": "9.0.0-beta.0",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "ejs-lint": "2.0.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "jiti": "2.6.1",
    "lint-staged": "16.2.7",
    "prettier": "3.7.4",
    "prettier-plugin-packagejson": "2.5.20",
    "vitest": "4.0.16",
    "yeoman-test": ">=10"
  },
  "engines": {
    "generator-jhipster": "9.0.0-beta.0",
    "node": "^20.18.3 || >=22.12.0"
  }
}

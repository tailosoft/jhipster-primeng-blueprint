import { I<%= entityAngularName %> } from './<%= entityFileName %>.model';

export const sampleWithRequiredData: I<%= entityAngularName %> = <%- this.generateTypescriptTestEntity([...fields.filter(field => field.id || field.fieldValidationRequired), ...relationships.filter(r => r.id)].map(field => field.reference)).replaceAll('dayjs(', 'new Date(') %>;

export const sampleWithPartialData: I<%= entityAngularName %> = <%- this.generateTypescriptTestEntity([...fields.filter(field => field.id || field.fieldValidationRequired || (!field.transient && faker.datatype.boolean())), ...relationships.filter(r => r.id)].map(field => field.reference)).replaceAll('dayjs(', 'new Date(') %> as any as I<%= entityAngularName %>;

export const sampleWithFullData: I<%= entityAngularName %> = <%- this.generateTypescriptTestEntity([...fields.filter(field => !field.transient), ...relationships.filter(r => r.id)].map(field => field.reference)).replaceAll('dayjs(', 'new Date(') %>;
<%_ if (primaryKey) { _%>

export const sampleWithNewData: I<%= entityAngularName %> = <%- this.generateTypescriptTestEntity([...fields.filter(field => !(field.id && field.autoGenerate) && field.fieldValidationRequired), ...relationships.filter(r => r.id)].map(field => field.reference)).replaceAll('dayjs(', 'new Date(') %> as any as I<%= entityAngularName %>;

Object.freeze(sampleWithNewData);
<%_ } _%>
Object.freeze(sampleWithRequiredData);
Object.freeze(sampleWithPartialData);
Object.freeze(sampleWithFullData);

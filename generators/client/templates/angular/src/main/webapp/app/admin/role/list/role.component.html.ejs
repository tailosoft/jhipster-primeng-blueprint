<div class="entity-list">
  <h2 class="title" id="page-heading" data-cy="RoleHeading">
    <span <%= jhiPrefix %>Translate="<%= frontendAppName %>.role.home.title">Roles</span>
  </h2>
  <div class="list-buttons">
    <p-button
      id="jh-create-entity"
      class="jh-create-entity create-role"
      [routerLink]="['/admin/role/new']"
      [label]="'<%= frontendAppName %>.role.home.createLabel' | translate"
      icon="pi pi-plus"
      iconPos="left"
      data-cy="entityCreateButton"
    >
    </p-button>
  </div>
  <div class="card list-content">
    <p-table #roleTable [value]="roles!" [rowTrackBy]="trackId" [responsive]="true">
      <ng-template pTemplate="header">
        <tr>
          <th>
            <span <%= jhiPrefix %>Translate="<%= frontendAppName %>.role.name">Name</span>
          </th>
          <th style="width: 124px"></th>
        </tr>
      </ng-template>
      <ng-template pTemplate="body" let-role>
        <tr data-cy="entityTable">
          <td>
            <a [routerLink]="['/admin/role', role.name, 'view']">{{ role.name }}</a>
          </td>
          <td>
            <div *ngIf="role.name !== 'ROLE_ADMIN'" class="btn-group flex-btn-group-container">
              <p-button
                type="submit"
                [routerLink]="['/admin/role', role.name!, 'edit']"
                icon="pi pi-pencil"
                styleClass="p-button-warning"
                data-cy="entityEditButton"
              >
              </p-button>
              <p-button
                type="submit"
                (click)="delete(role.name)"
                icon="pi pi-times"
                styleClass="p-button-danger"
                data-cy="entityDeleteButton"
              >
              </p-button>
            </div>
          </td>
        </tr>
      </ng-template>
    </p-table>
  </div>
</div>
